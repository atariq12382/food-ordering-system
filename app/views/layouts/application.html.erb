<!DOCTYPE html>
<html>
  <head>
    <title>FoodOrderingSystem</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_pack_tag 'application', 'data-turbolinks-track': 'reload' %>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <link rel="icon" href="favicon.ico">
    <!-- Simple bar CSS -->
    <%= stylesheet_link_tag 'simplebar.css' %>
    <!-- Fonts CSS -->
    <link href="https://fonts.googleapis.com/css2?family=Overpass:ital,wght@0,100;0,200;0,300;0,400;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
    <!-- Icons CSS -->
    <%= stylesheet_link_tag 'feather.css' %>
    <%= stylesheet_link_tag 'select2.css' %>
    <%= stylesheet_link_tag 'dropzone.css' %>
    <%= stylesheet_link_tag 'uppy.min.css' %>
    <%= stylesheet_link_tag 'jquery.steps.css' %>
    <%= stylesheet_link_tag 'jquery.timepicker.css' %>
    <%= stylesheet_link_tag 'quill.snow.css' %>
    <!-- Date Range Picker CSS -->
    <%= stylesheet_link_tag 'daterangepicker.css' %>
    <%# <!-- App CSS -->
    <%= stylesheet_link_tag 'app-light.css', id: 'lightTheme'%> %>
    <%= stylesheet_link_tag 'app-dark.css', id: "darkTheme" %>
  </head>

  <body>
    <%= render 'layouts/notifications' %>
    <%= yield %>
  </body>
</html>



                        